Frame "SIMPLEFRAME" "SimpleInfoPanelIconMoveSpeed" {
    UseActiveContext,
    SetAllPoints,
    DecorateFileNames,
    Height 0.03125,
    Width 0.03125,

    // --- icon -------------------------------------------------------------
    Texture "InfoPanelIconMoveSpeedBackdrop" INHERITS "InfoPanelIconTemplate" {
    }

    // --- icon # -----------------------------------------------------------
	String "InfoPanelIconMoveSpeedLevel" INHERITS "SimpleInfoPanelAttributeTextTemplate" {
        SetPoint CENTER, "InfoPanelIconMoveSpeedBackdrop", BOTTOMRIGHT, -0.007625, 0.006875,
	}

    // --- value ------------------------------------------------------------
    String "InfoPanelMoveSpeedIconValue" INHERITS "SimpleInfoPanelValueTextTemplate" {
         SetPoint TOPLEFT, "InfoPanelIconMoveSpeedBackdrop", TOPRIGHT, 0.0, -0.005,
    }
}

